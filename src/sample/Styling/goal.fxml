<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<fx:root prefHeight="48.0" prefWidth="495.0" stylesheets="@style.css" type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ProgressBar fx:id="progressBar" layoutX="175.0" layoutY="10.0" onMouseClicked="#showNextProgressView" prefWidth="200.0" progress="0.0" />
      <Button fx:id="addToProgress" layoutX="381.0" layoutY="6.0" mnemonicParsing="false" onAction="#increaseProgress" prefHeight="25.0" prefWidth="25.0" text="+" />
      <Button fx:id="removeFromProgress" layoutX="413.0" layoutY="6.0" mnemonicParsing="false" onAction="#decreaseProgress" prefHeight="25.0" prefWidth="25.0" text="-" />
      <Label fx:id="goalLabel" layoutX="20.0" layoutY="10.0" prefHeight="17.0" prefWidth="135.0" styleClass="cText" text="Goal" wrapText="true" />
      <Label fx:id="percentageLabel" layoutX="285.0" layoutY="32.0" styleClass="goalText" text="\%" />
      <Label fx:id="progressLabel" layoutX="330.0" layoutY="30.0" prefHeight="19.0" prefWidth="101.0" styleClass="goalText" text="5/20">
         <font>
            <Font name="DejaVu Sans Condensed Bold" size="12.0" />
         </font></Label>
      <Label fx:id="goalDescLabel" layoutX="175.0" layoutY="30.0" styleClass="dText" text="Yearly Goal" />
   </children>
</fx:root>
