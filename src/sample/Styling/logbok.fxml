<fx:root prefHeight="165.0" prefWidth="260.0" style="-fx-background-color: #383838;" stylesheets="@style.css" type="AnchorPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
