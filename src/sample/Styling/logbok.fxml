            <Insets left="2.0" right="2.0" />
         </padding>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></TextArea>
      <Label fx:id="scoreLabel" layoutX="208.0" prefHeight="30.0" prefWidth="37.0" styleClass="cText" text="6/10" />
      <Label fx:id="dateLabel" layoutX="8.0" layoutY="7.0" styleClass="cText" text="8/4" />
   </children>
